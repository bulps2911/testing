{
  "name": "testing",
  "jest": {
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)?$": "<rootDir>/jest-transform.js"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@testing-library/react": "^9.4.0",
    "axios": "^0.19.0",
    "cra-template": "1.0.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "react-test-renderer": "^16.12.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "react-addons-test-utils": "^15.6.2",
    "ts-jest": "^24.2.0"
  }
}
